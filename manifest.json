{
  "manifest_version": 3,
  "name": "TradingView Auto Logger",
  "version": "1.0",
  "description": "Logs something when TradingView is opened",
  "permissions": ["storage", "scripting", "tabs"],
  "host_permissions": ["*://*.tradingview.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.tradingview.com/*"],
      "js": ["content.js"]
    }
  ]
}
